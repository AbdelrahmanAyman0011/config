return {
  "catppuccin/nvim",
  name = "catppuccin",
  lazy = false,
  priority = 1000,
  config = function()
    require("catppuccin").setup({
      flavour = "macchiato",
      transparent_background = false, -- Set to false for light theme
    })
    vim.cmd([[colorscheme catppuccin-macchiato]])
  end,
}
